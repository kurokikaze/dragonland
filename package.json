{
  "name": "dragonland",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "cjs": true,
  "scripts": {
    "watch": "parcel watch 'client/game.js' --dist-dir public/javascripts --no-cache",
    "build": "parcel build 'client/game.js' --dist-dir public/javascripts --no-cache",
    "watch-challenge": "parcel watch challenge/index.js --dist-dir public/javascripts --no-cache",
    "build-challenge": "parcel build './challenge/index.js' --dist-dir public/javascripts --no-cache",
    "download-cards": "node  --es-module-specifier-resolution=node ./bin/download.js",
    "setup-database": "node ./bin/setupDb.js",
    "start": "node --es-module-specifier-resolution=node ./bin/www.js",
    "start-prod": "pm2 start www --restart-delay=3000",
    "restart-prod": "pm2 restart www --restart-delay=3000",
    "test": "jest --no-cache",
    "storybook": "start-storybook -p 6006 -s ./public/",
    "build-storybook": "build-storybook -s ./public/"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@types/classnames": "^2.2.11",
    "@types/nanoid": "^2.1.0",
    "@types/react": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/recompose": "^0.30.7",
    "antd": "^4.9.2",
    "babel-jest": "^26.3.0",
    "classnames": "^2.2.6",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cross-fetch": "^3.0.6",
    "debug": "~2.6.9",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.2",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "g": "^2.0.1",
    "hookrouter": "^1.2.5",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^0.20.0",
    "mongodb": "^3.6.2",
    "moonlands": "https://github.com/kurokikaze/moonlands/archive/v0.6.2.tar.gz",
    "morgan": "~1.9.1",
    "nanoid": "^2.1.9",
    "node-fetch": "^2.6.1",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.1",
    "ramda": "^0.26.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.3.0",
    "style.css": "^1.0.0",
    "superagent": "^6.1.0",
    "supertest": "^4.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^6.2",
    "@storybook/addon-essentials": "^6.2",
    "@storybook/addon-links": "^6.2",
    "@storybook/react": "^6.2",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "parcel": "^2.0.0-beta.3.1",
    "@parcel/transformer-typescript-tsc": "2.0.0-beta.3.1",
    "react": "^17",
